{
  "name": "ambher",
  "version": "1.0.0",
  "description": "Backend Repository of Ambher",
  "main": "app.js",
  "scripts": {
    "dev": "export NODE_ENV=development && nodemon app.js",
    "win-dev": "set NODE_ENV=development && nodemon app.js",
    "start": "node app.js",
    "test": "set NODE_ENV=test && nyc --reporter=html mocha --recursive ./test --timeout 10000 --exit",
    "test-single": "npx mocha --timeout 10000 --exit",
    "win-test": "set NODE_ENV=test && nyc --reporter=html mocha --recursive ./test --timeout 10000 --exit",
    "check-linting": "npx eslint config src test app.js",
    "fix-linting": "npx eslint config src test app.js --fix"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ambher-Backend/Ambher-Backend-Phase-1"
  },
  "bugs": {
    "url": "https://github.com/Ambher-Backend/Ambher-Backend-Phase-1/issues"
  },
  "author": "Ambher",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^4.1.2",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "dotenv": "^12.0.4",
    "express": "^4.17.2",
    "faker": "^5.5.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.12.1",
    "mongoose-fuzzy-searching": "^2.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.0",
    "nodemailer": "^6.7.2",
    "path": "^0.12.7",
    "socket.io": "^4.0.0",
    "uuid": "^8.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "eslint": "^8.8.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2"
  }
}
